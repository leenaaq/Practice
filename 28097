#include <iostream>
using namespace std;

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);

    int n, t, sum = 0; cin >> n;
    for (int i = 0; i < n; i++) {
        cin >> t;
        sum += t;
        if (i < n-1) sum += 8;
    }
    cout << sum / 24 << " " << sum % 24;

    

    return 0;
}
