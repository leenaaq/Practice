#include <iostream>
using namespace std;

int a() {
    int a; cin >> a;
    if (a % 2 == 0) {
        cout << a << " is even\n";
        return 0;
    }
    else {
        cout << a << " is odd\n";
        return 0;
    }
}


int main() {
    int n; cin >> n;
    for (int i = 0; i < n; i++) {
        a();
    }
    return 0;
}
