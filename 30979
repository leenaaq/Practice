#include <iostream>
using namespace std;

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);

    int t, n, f; cin >> t >> n;
    for (int i = 0; i < n; i++) {
        cin >> f;
        t -= f;
    }
    if (t <= 0) cout << "Padaeng_i Happy";
    else cout << "Padaeng_i Cry";

    return 0;
}
