#include <iostream>
using namespace std;

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);

    int n, c = 0;
    cin >> n;

    int a[1000], b[1000];
    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }

    for (int i = 0; i < n; i++) {
        cin >> b[i];
    }

    for (int i = 0; i < n; i++) {
        if (a[i] <= b[i]) {
            c++;
        }
    }

    cout << c;

    return 0;
}
