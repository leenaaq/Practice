#include <iostream>
using namespace std;

void ah(void) {
	int s, k, h; cin >> s >> k >> h;
	if (s + k + h >= 100) cout << "OK";
	else {
		if (s < k && s < h) cout << "Soongsil";
		else if (k < s && k < h) cout << "Korea";
		else if (h < s && h < k) cout << "Hanyang";
	}
}

int main(void) {
	ios::sync_with_stdio(false);
	cin.tie(nullptr); cout.tie(nullptr);

	ah();
	return 0;
}
