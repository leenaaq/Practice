#include <iostream>

int main() {
    std::cout << "  ___  ___  ___\n";
    std::cout << "  | |__| |__| |\n";
    std::cout << "  |           |\n";
    std::cout << "   \\_________/\n";
    std::cout << "    \\_______/\n";
    std::cout << "     |     |\n";
    std::cout << "     |     |\n";
    std::cout << "     |     |\n";
    std::cout << "     |     |\n";
    std::cout << "     |_____|\n";
    std::cout << "  __/       \\__\n";
    std::cout << " /             \\\n";
    std::cout << "/_______________\\\n";

    return 0;
}
