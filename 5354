#include <iostream>
using namespace std;

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);

    int n, a; cin >> n;

    while (n--) {
        cin >> a;
        for (int i = 0; i < a; i++) {
            for (int j = 0; j < a; j++) {
                if (i == 0 || i == a - 1 || j == 0 || j == a - 1) cout << "#";
                else cout << "J";
            }
            cout << "\n";
        }
        cout << "\n";
    }
    return 0;
}
