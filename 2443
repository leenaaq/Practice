#include <iostream>

int main() {
    int a, b = 0;
    std::cin >> a;

    for (int i = 0; i < a; i++) {
        for (int j = 0; j < b; j++) std::cout << " ";
        for (int j = 0; j < a - i; j++) {
            std::cout << "*";
        }
        for (int j = 0; j < a - i - 1; j++) {
            std::cout << "*";
        }
        b++;
        std::cout << "\n";
    }
    
}
