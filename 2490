#include <iostream>

int main() {
    int a[3][4];

    for (int i = 0; i < 3; i++) {
        int sum = 0;
        for (int j = 0; j < 4; j++) {
            std::cin >> a[i][j];
            sum += a[i][j];
        }
        if (sum == 3) std::cout << "A" << "\n";
        else if (sum == 2) std::cout << "B" << "\n";
        else if (sum == 1) std::cout << "C" << "\n";
        else if (sum == 0) std::cout << "D" << "\n";
        else if (sum == 4) std::cout << "E" << "\n";
    }
}
