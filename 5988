#include <iostream>
#include <string>
using namespace std;

int main()
{
    int n; cin >> n;
    string k;
    int a;
    for (int i = 0; i < n; i++) {
        cin >> k;
        a = (k[k.size() - 1]) - '0';
        if (a % 2 == 0) cout << "even\n";
        else cout << "odd\n";
    }
    
    return 0;
}

