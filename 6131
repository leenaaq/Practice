#include <iostream>
using namespace std;

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);

    int n, sum = 0; cin >> n;

    for (int i = 1; i <= 500; i++) {
        for (int j = 1; j <= 500; j++) {
            if (i * i + n == j * j) sum++;
        }
    }
    cout << sum;    

    return 0;
}
