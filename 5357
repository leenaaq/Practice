#include <iostream>
using namespace std;

void ah(void) {
	int n;
	string a;
	cin >> n;
	for (int i = 0; i < n; i++) {
		cin >> a;
		for (int j = 0; j < a.length(); j++) {
			if (a[j] != a[j + 1]) cout << a[j];
		}
		cout << "\n";
	}
}

int main(void) {
	ios::sync_with_stdio(false);
	cin.tie(nullptr); cout.tie(nullptr);

	ah();
	return 0;
}
