	#include <iostream>
	using namespace std;

	int main() {
		ios::sync_with_stdio(0);
		cin.tie(0);
		
		string str; cin >> str;
		bool a = 0;
		for (int i = 0; i <= str.length(); i++) {
			if (str[i] == 's' && str[i + 1] == 's') a = 1;
		}

		if (a == 1) cout << "hiss";
		else cout << "no hiss";
		return 0;
	}
