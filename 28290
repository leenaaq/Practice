#include <iostream>
using namespace std;

void result(string str) {
    if (str == "fdsajkl;" || str == "jkl;fdsa") cout << "in-out";
    else if (str == "asdf;lkj" || str == ";lkjasdf") cout << "out-in";
    else if (str == "asdfjkl;") cout << "stairs";
    else if (str == ";lkjfdsa") cout << "reverse";
    else cout << "molu";
}

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);

    string str; cin >> str;
    result(str);
    return 0;
}
